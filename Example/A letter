from turtle import *
s = Screen()
s.setup(600,600)
s#.bgcolor('black')
#pencolor('red')
t = Turtle()
speed('fast')
pensize(5)
t.lt(70) 
t.fd(150)
t.rt(140)
t.fd(150)
t.rt(90)
t.fd(20)
t.rt(90)
t.fd(60)
t.lt(80)
t.fd(30)
t.rt(180)
t.fd(30)
t.lt(100)
t.fd(40)
t.lt(140)
t.fd(100)
t.rt(90)
t.fd(20)
mainloop()